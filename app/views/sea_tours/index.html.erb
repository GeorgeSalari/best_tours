<p id="notice"><%= notice %></p>

<h1>Sea Tours</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sea_tours.each do |sea_tour| %>
      <tr>
        <td><%= link_to 'Show', sea_tour %></td>
        <td><%= link_to 'Edit', edit_sea_tour_path(sea_tour) %></td>
        <td><%= link_to 'Destroy', sea_tour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sea Tour', new_sea_tour_path %>
